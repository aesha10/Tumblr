<div id="page_wrapper">
  <h1>Make Something!</h1>

  <%= form_for :post, url: posts_path do |f| %>
    <p>
      <%= f.label :title %> <br>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :body %> <br>
      <%= f.text_area :body %>
    </p>
    <button class="btn btn-default">
      <%= f.submit %>
    </button>
  <% end %>
 </div>